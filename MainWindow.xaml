<Window x:Class="LearnWinInterop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LearnWinInterop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center" Orientation="Horizontal"
                HorizontalAlignment="Center">
        <Button x:Name="_WordSavebtn_" Width="250" Height="50" Content="Сохранить в Word" Click="_WordSavebtn__Click"/>
        <Button x:Name="_ExcelSavebtn_" Width="250" Height="50" Content="Сохранить в Excel" Click="_ExcelSavebtn__Click"/>
    </StackPanel>
</Window>
